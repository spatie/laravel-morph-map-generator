{
    "name" : "spatie/laravel-morph-map-generator",
    "description" : "Automatically generate morph maps in your Laravel app",
    "keywords" : [
        "spatie",
        "laravel-morph-map-generator"
    ],
    "homepage" : "https://github.com/spatie/laravel-morph-map-generator",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Ruben Van Assche",
            "email" : "ruben@spatie.be",
            "homepage" : "https://spatie.be",
            "role" : "Developer"
        }
    ],
    "require" : {
        "php": "^7.4",
        "ext-json": "*",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev" : {
        "friendsofphp/php-cs-fixer" : "^2.16",
        "orchestra/testbench" : "^6.0",
        "phpunit/phpunit" : "^9.3",
        "vimeo/psalm" : "^3.11",
        "spatie/temporary-directory": "^1.2"
    },
    "autoload" : {
        "psr-4" : {
            "Spatie\\LaravelMorphMapGenerator\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Spatie\\LaravelMorphMapGenerator\\Tests\\" : "tests"
        }
    },
    "scripts" : {
        "psalm" : "vendor/bin/psalm",
        "test" : "vendor/bin/phpunit --colors=always",
        "test-coverage" : "vendor/bin/phpunit --coverage-html coverage",
        "format" : "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config" : {
        "sort-packages" : true
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Spatie\\LaravelMorphMapGenerator\\LaravelMorphMapGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "funding" : [
        {
            "type" : "github",
            "url" : "https://github.com/sponsors/spatie"
        },
        {
            "type" : "other",
            "url" : "https://spatie.be/open-source/support-us"
        }
    ]
}
